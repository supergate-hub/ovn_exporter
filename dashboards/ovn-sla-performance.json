      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          },
          "editorMode": "code",
          "expr": "sum(max by (system_id) (rate(ovn_failed_requests_total[5m]))) / clamp_min(sum(max by (system_id) (rate(ovn_requests_total[5m]))), 0.001)",
          "legendFormat": "Error Rate",
          "range": true,
          "refId": "A"
        }
      ],
      "title": "Error Rate",
      "type": "stat"
    },
    {
